      "source": [
        "import os\n",
        "import pickle\n",
        "from langchain_classic.chains import RetrievalQA\n",
        "from transformers import pipeline\n",
        "from langchain_community.llms import HuggingFacePipeline\n",
        "from dotenv import load_dotenv\n",
        "from langchain_community.document_loaders import UnstructuredURLLoader\n",
        "from langchain_community.embeddings import HuggingFaceEmbeddings # Corrected import\n",
        "from langchain_community.vectorstores import FAISS # Corrected import for FAISS\n",
        "from langchain_text_splitters import CharacterTextSplitter\n",
        "import streamlit as st\n",
        "\n",
        "# Load environment variables\n",
        "load_dotenv()\n",
        "\n",
        "# --- Model Configuration ---\n",
        "MODEL_NAME = \"TinyLlama/TinyLlama-1.1B-Chat-v1.0\"\n",
        "# Note: For significantly faster initial loading and inference, consider a smaller model\n",
        "# e.g., \"microsoft/Phi-3-mini-4k-instruct\" or \"distilbert-base-uncased\" for embeddings.\n",
        "\n",
        "@st.cache_resource\n",
        "def get_llm():\n",
        "    \"\"\"Initializes and caches the Hugging Face LLM pipeline.\n",
        "    This function will run only once across Streamlit reruns.\"\"\"\n",
        "    with st.spinner(f\"Loading LLM model: {MODEL_NAME}... This may take several minutes for the first download and load.\"):\n",
        "        # Ensure that device mapping is handled if a GPU is available\n",
        "        device = 0 if os.environ.get(\"CUDA_VISIBLE_DEVICES\", \"\") != \"\" else -1\n",
        "        hf_pipeline = pipeline(\n",
        "            task=\"text-generation\",\n",
        "            model=MODEL_NAME,\n",
        "            max_new_tokens=512,\n",
        "            temperature=0.7,\n",
        "            do_sample=True,\n",
        "            device=device # Use GPU if available\n",
        "        )\n",
        "        return HuggingFacePipeline(pipeline=hf_pipeline)\n",
        "\n",
        "@st.cache_resource\n",
        "def get_embedding_model():\n",
        "    \"\"\"Initializes and caches the Hugging Face embedding model.\n",
        "    This function will run only once across Streamlit reruns.\"\"\"\n",
        "    with st.spinner(\"Loading embedding model (sentence-transformers/all-MiniLM-L6-v2)...\"):\n",
        "        return HuggingFaceEmbeddings(model_name=\"sentence-transformers/all-MiniLM-L6-v2\")\n",
        "\n",
        "llm = get_llm()\n",
        "embedding_model = get_embedding_model()\n",
        "\n",
        "# --- Streamlit UI ---\n",
        "st.title(\"News Analyzer📈\")\n",
        "st.sidebar.title(\"News article urls\")\n",
        "\n",
        "urls_input = []\n",
        "for i in range(3):\n",
        "    urls_input.append(st.sidebar.text_input(f\"URL{i+1}\", key=f\"url_input_{i+1}\"))\n",
        "\n",
        "process_url_clicked = st.sidebar.button(\"Process URLs\")\n",
        "file_path = 'faiss_store.pkl'\n",
        "main_placeholder = st.empty()\n",
        "\n",
        "if process_url_clicked:\n",
        "    valid_urls = [url for url in urls_input if url]\n",
        "    if not valid_urls:\n",
        "        main_placeholder.error(\"Please enter at least one URL to process.\")\n",
        "    else:\n",
        "        with st.spinner(\"Loading data from URLs...\"):\n",
        "            loader = UnstructuredURLLoader(urls=valid_urls)\n",
        "            data = loader.load()\n",
        "            if not data: # Added check for empty data\n",
        "                main_placeholder.warning(\"No content could be loaded from the provided URLs. Please check the URLs and try again.\")\n",
        "                st.stop() # Stop execution if no data\n",
        "            main_placeholder.info(f\"Loaded {len(data)} documents.\")\n",
        "\n",
        "        with st.spinner(\"Splitting text into chunks...\"):\n",
        "            text_splitter = CharacterTextSplitter(\n",
        "                separator='\\\\n\\\\n|\\\\n|\\\\.|,',\n",
        "                chunk_size=200,\n",
        "                chunk_overlap=0\n",
        "            )\n",
        "            chunks = text_splitter.split_documents(data)\n",
        "            if not chunks: # Added check for empty chunks\n",
        "                main_placeholder.warning(\"No text chunks could be created from the loaded documents. The content might be too short or unparseable.\")\n",
        "                st.stop() # Stop execution if no chunks\n",
        "            main_placeholder.info(f\"Split into {len(chunks)} chunks.\")\n",
        "\n",
        "\n",
        "        with st.spinner(\"Creating data embeddings and building FAISS index... This can take a moment.\"):\n",
        "            vectorstore = FAISS.from_documents(chunks, embedding_model)\n",
        "\n",
        "        with st.spinner(\"Saving FAISS index to faiss_store.pkl...\"):\n",
        "            with open(file_path, 'wb') as f:\n",
        "                pickle.dump(vectorstore, f)\n",
        "        main_placeholder.success(\"FAISS index saved to faiss_store.pkl. You can now ask questions!\")\n",
        "\n",
        "query = main_placeholder.text_input(\"Ask a question about the articles:\")\n",
        "\n",
        "if query:\n",
        "   if os.path.exists(file_path):\n",
        "     with  open(file_path, 'rb') as f:\n",
        "        vectorstore = pickle.load(f)\n",
        "        main_placeholder.info(\"FAISS index loaded. Generating answer...\")\n",
        "        chain = RetrievalQA.from_llm(llm=llm, retriever=vectorstore.as_retriever())\n",
        "        result = chain({'query': query})\n",
        "        st.header('Answer')\n",
        "        st.subheader(result['result'])\n",
        "   else:\n",
        "     main_placeholder.warning(\"FAISS index not found. Please process URLs first by clicking 'Process URLs'.\")\n"
      ],
